{
    "name": "antv-x6-vue2",
    "version": "1.0.0",
    "description": "antv/x6 图形化组件",
    "private": false,
    "main": "lib/antv.common.js",
    "typings": "types/index.d.ts",
    "files": [
        "lib",
        "types/*.d.ts"
    ],
    "license": "MIT",
    "scripts": {
        "svgo": "svgo -f packages/x6/icons/svg --config=packages/x6/icons/svgo.yml",
        "build": "vue-cli-service build --target lib --name antv --dest lib packages/index.js",
        "serve": "vue-cli-service serve",
        "report": "vue-cli-service build --report",
        "build:types": "tsc -p .",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "@antv/x6": "^1.24.8",
        "@antv/x6-vue-shape": "^1.2.8",
        "core-js": "^3.6.5",
        "element-ui": "2.14.1",
        "vue": "^2.6.11"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/composition-api": "^1.0.5",
        "babel-eslint": "^10.1.0",
        "babel-plugin-component": "^1.1.1",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "sass": "1.26.5",
        "sass-loader": "8.0.2",
        "svg-sprite-loader": "4.2.1",
        "svgo": "1.3.2",
        "uglifyjs-webpack-plugin": "2.2.0",
        "vue-router": "^3.5.2",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}